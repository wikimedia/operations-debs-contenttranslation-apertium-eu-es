    <def-macro n="ser_tener" npar="2"><!--ser o tener, esta es la cuestión :-)
                Esta macro mira si el auxiliar es transitivo y si el verbo principal es 'izan' (que se traduce por 'ser' al castellano). En caso afirmativo, cambia la traducción de 'ser' a 'tener' (izango duzue - seréis - tendréis.)
Podría ampliarse a otros casos, para tratar las diferencias en la traducción de los verbos según si el auxiliar es transitivo o intransitivo.

 -->
	<choose>
	  <when>
	    <test>
	      <and>
		<equal caseless="yes">
		  <clip pos="1" side="tl" part="lemh"/>
		  <lit v="ser"/>
		</equal>
		<not>
		  <equal caseless="yes">
		    <clip pos="2" side="sl" part="nork"/>
		    <lit v=""/>
		  </equal>
		</not>
	      </and>
	    </test>
	    <let>
	      <clip pos="1" side="tl" part="lemh"/>
	      <get-case-from pos="1">
		<lit v="tener"/>
	      </get-case-from>
	    </let>
	    <let>
	      <clip pos="1" side="tl" part="a_verb"/>
	      <lit-tag v="vblex"/>
	    </let>
	  </when>
	</choose>
</def-macro>


