    <def-macro n="SPgenitivo" npar="1"><!--si la postposición és ko/en, el nombre del chunk serà SPGEN para que el t2x realice las operaciones necesarias. Nota: hay reglas que detectan ya chunks con CN dentro ('aitaren etxea'), la marca de genitivo es para los chunks que escapan a estas reglas, para que el t2x pueda tratarlos. -->

      <choose>
	<when>
	  <test>
	    <or>
	      <equal>		
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="en"/>
	      </equal>
	      <equal>		
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="ko"/>
	      </equal>
	      <equal>		
		<clip pos="1" side="sl" part="a_post"/> 
		<lit-tag v="ko"/>
	      </equal>
	    </or>
	  </test>
	  <let>
	    <var n="chunk"/>
	    <lit-tag v="SPGEN"/>
	  </let>
	</when>
	<when>
	  <test>
	    <or>
	      <equal>
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="k"/>
	      </equal>
	      <equal>
		<clip pos="1" side="sl" part="lem"/> 
		<lit v="ik"/>
	      </equal>
	    </or>
	  </test>
	  <let>
	    <var n="chunk"/>
	    <lit-tag v="SN"/>
	  </let>
	</when>
	<otherwise>
	  <let>
	    <var n="chunk"/>
	    <lit-tag v="SPR"/>
	  </let>
	</otherwise>
      </choose>
      <choose><!-- si la postposición es 'k', como la traducción es vacía, se elimina el blanco. El blanco después de la postposición hay que enviarlo con la variable 'blanco'-->
	<when>
